
@{
    ViewBag.Title = "FloorRentalOnly";
    int counter = 0;
}

<h2>FloorRentalOnly</h2>
@Html.ActionLink("Return To Home", "Index", "Home")

<table width="100%" border="1">
    <tr><th>Name</th><th>Level</th></tr>
    @foreach(OKSwingMemberList mem in ViewBag.FloorRentalOnly)
    {
        <tr><td>@mem.LastName, @mem.FirstName</td><td>FLOOR RENTAL ONLY</td></tr>
        counter++;
    }
    <tr><td>Total Number Of Dancers:</td><td>@counter</td></tr>
</table>

